        "wotr-bundler": {
            "root": "apps/wotr/bundler",
            "sourceRoot": "apps/wotr/bundler/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/node:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/wotr/bundler",
                        "main": "apps/wotr/bundler/src/main.ts",
                        "tsConfig": "apps/wotr/bundler/tsconfig.app.json",
                        "assets": ["apps/wotr/bundler/src/assets"]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/wotr/bundler/src/environments/environment.ts",
                                    "with": "apps/wotr/bundler/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "wotr-bundler:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": ["apps/wotr/bundler/**/*.ts"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/wotr/bundler"],
                    "options": {
                        "jestConfig": "apps/wotr/bundler/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            },
            "tags": []
        },
        "wotr-explorer": {
            "projectType": "application",
            "root": "apps/wotr/explorer",
            "sourceRoot": "apps/wotr/explorer/src",
            "prefix": "myth-tools",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/wotr/explorer",
                        "index": "apps/wotr/explorer/src/index.html",
                        "main": "apps/wotr/explorer/src/main.ts",
                        "polyfills": "apps/wotr/explorer/src/polyfills.ts",
                        "tsConfig": "apps/wotr/explorer/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": ["apps/wotr/explorer/src/favicon.ico", "apps/wotr/explorer/src/assets"],
                        "styles": ["apps/wotr/explorer/src/styles.scss"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "apps/wotr/explorer/src/environments/environment.ts",
                                    "with": "apps/wotr/explorer/src/environments/environment.prod.ts"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "wotr-explorer:build:production"
                        },
                        "development": {
                            "browserTarget": "wotr-explorer:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "wotr-explorer:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/wotr/explorer/src/**/*.ts", "apps/wotr/explorer/src/**/*.html"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/wotr/explorer"],
                    "options": {
                        "jestConfig": "apps/wotr/explorer/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            },
            "tags": []
        },
        "wotr-explorer-e2e": {
            "root": "apps/wotr/explorer-e2e",
            "sourceRoot": "apps/wotr/explorer-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/wotr/explorer-e2e/cypress.json",
                        "devServerTarget": "wotr-explorer:serve:development"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "wotr-explorer:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": ["apps/wotr/explorer-e2e/**/*.{js,ts}"]
                    }
                }
            },
            "tags": [],
            "implicitDependencies": ["wotr-explorer"]
        },

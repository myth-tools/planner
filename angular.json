                    "options": {
                        "browserTarget": "wotr-blueprints:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/wotr-blueprints/src/**/*.ts", "apps/wotr-blueprints/src/**/*.html"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/wotr-blueprints"],
                    "options": {
                        "jestConfig": "apps/wotr-blueprints/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            },
            "tags": []
        },
        "wotr-compiler": {
            "root": "apps/wotr-compiler",

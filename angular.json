                            "repo": "https://github.com/myth-tools/wotr.git",
                            "name": "Christopher",
                            "email": "24984762+christopher-kiss@users.noreply.github.com"
                        }
                    }
                }
            },
            "tags": []
        },
        "wotr-planner-e2e": {
            "root": "apps/wotr/planner-e2e",
            "sourceRoot": "apps/wotr/planner-e2e/src",
